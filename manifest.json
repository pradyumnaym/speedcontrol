{
    "manifest_version": 2,
    
    "name": "SpeedControl",
    "content_scripts": [
        {
          "matches": ["http://*/*","https://*/*"],
          "js": ["contentScript.js"]
        }
      ],
    "description": "This extension will provide a smooth video playback rate control",
    "version": "1.0",
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "permissions": [
     "activeTab"
     ]
  }